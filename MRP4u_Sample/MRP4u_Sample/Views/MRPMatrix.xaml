<UserControl x:Class="MRP4ME_Sample.Views.MRPMatrix"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MRP4ME_Sample.Views"
             xmlns:rpt="clr-namespace:Microsoft.Reporting.WinForms;assembly=Microsoft.ReportViewer.WinForms"
             mc:Ignorable="d" >
    <Grid>
        <WindowsFormsHost HorizontalAlignment="Left" VerticalAlignment="Top" Height="500" Width="1000">
            <rpt:ReportViewer x:Name="rptName"/>
        </WindowsFormsHost>

    </Grid>
</UserControl>
